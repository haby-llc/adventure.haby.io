    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "chai": "^4.3.4",
    "eslint": "8.4.1",
    "eslint-config-next": "12.0.7",
    "ethereum-waffle": "^3.4.0",
    "hardhat": "^2.6.5"
  }
