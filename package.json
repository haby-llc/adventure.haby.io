  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "start": "next start -H 0.0.0.0 -p ${PORT:-8080}",
    "build": "next build",
    "test": "next test",
    "lint": "next lint",
    "contracts:compile": "npx hardhat compile",
    "contracts:serve": "npx hardhat node",
